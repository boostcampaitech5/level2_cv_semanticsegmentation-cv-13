ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /opt/conda/envs/internimage/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /opt/conda/envs/internimage/include -I/opt/conda/envs/internimage/include -fPIC -O2 -isystem /opt/conda/envs/internimage/include -fPIC -DWITH_CUDA -I/opt/ml/input/InternImage/segmentation/ops_dcnv3/src -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include/TH -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/conda/envs/internimage/include/python3.9 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=DCNv3 -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -DWITH_CUDA -I/opt/ml/input/InternImage/segmentation/ops_dcnv3/src -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include/torch/csrc/api/include -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include/TH -I/opt/conda/envs/internimage/lib/python3.9/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/conda/envs/internimage/include/python3.9 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=DCNv3 -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_70,code=compute_70 -gencode=arch=compute_70,code=sm_70 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





